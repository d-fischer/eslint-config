{
  "name": "@d-fischer/eslint-config",
  "version": "6.2.1",
  "description": "My personal ESLint config.",
  "repository": {
    "type": "git",
    "url": "https://github.com/d-fischer/eslint-config.git"
  },
  "author": "Daniel Fischer <daniel@d-fischer.dev>",
  "funding": "https://github.com/sponsors/d-fischer",
  "license": "MIT",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsdoc": "^46.8.2"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "husky": "^4.3.6",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "typescript": "~4.9.5"
  },
  "peerDependencies": {
    "eslint": "^8.0.1",
    "typescript": ">=4.5.2"
  },
  "files": [
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prettier:check": "prettier --check index.js",
    "prettier:fix": "prettier --write index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,css,json,md}": "prettier --write"
  }
}
